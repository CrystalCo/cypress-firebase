{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6"
        }
      }
    ]
  ],
  "plugins": [
    "lodash",
    "add-module-exports"
  ],
  "env": {
    "es": {
      "comments": false,
      "minified": false
    },
    "commonjs": {
      "comments": false,
      "minified": true
    }
  }
}
